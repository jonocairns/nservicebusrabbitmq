<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="RabbitMqTransportConfig" type="NServiceBus.Unicast.Transport.RabbitMQ.Config.RabbitMqTransportConfig, NServiceBus.RabbitMQ"/>
		<section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
	</configSections>

	<!--<appSettings>
		<add key="AMQP_PROTOCOL" value="AMQP_0_8"/>
	</appSettings>-->

	<RabbitMqTransportConfig 
		InputQueue="rmq://precision11-VirtualBox/helloEx/c1" 
		ErrorQueue="rmq://precision11-VirtualBox//errors" 
		NumberOfWorkerThreads="1" 
		MaxRetries="3"
		SendAcknowledgement="false"
		ListenExchangeType="fanout"/>

	<UnicastBusConfig>
		<MessageEndpointMappings>
		</MessageEndpointMappings>
	</UnicastBusConfig>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
</configuration>
